{
  "name": "sivera",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "next dev --turbopack",
    "build": "next build",
    "build:skip-types": "NEXT_SKIP_TYPE_CHECK=true next build",
    "start": "next start",
    "start:local": "next start",
    "start:supabase": "npx supabase start && npx supabase link && npx supabase db pull",
    "local": "npm run start:supabase && npm run dev:local",
    "tsc": "npx tsc --noEmit --project .",
    "typecheck": "npx tsc --noEmit --project .",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write .",
    "pre-commit": "lint-staged",
    "commitlint": "commitlint --edit",
    "test": "pnpm typecheck && pnpm lint && pnpm format && pnpm test:unit && pnpm build",
    "test:unit": "NODE_ENV=test NO_SERVER=true playwright test --project=unit-tests --reporter=list",
    "test:debug": "NODE_ENV=test playwright test --debug --headed --workers=1",
    "test:watch": "NODE_ENV=test playwright test --ui",
    "test:db": "supabase test db",
    "gen:type:supabase": "npx supabase gen types typescript --linked > types/supabase.types.ts",
    "gen:type:supabase:local": "npx supabase gen types typescript --local > types/supabase.types.ts",
    "db:migrate": "npx supabase migration up --linked --include-all",
    "db:migrate:local": "npx supabase migration up --local --include-all",
    "func:deploy": "npx supabase functions deploy  resend --project-ref dtttnnwrzbtjzjzmpvaf ",
    "prepare": "husky",
    "test:component": "NODE_ENV=test playwright test -c playwright-ct.config.ts --reporter=list",
    "test:integration": "NODE_ENV=test playwright test --project=integration-tests --reporter=list",
    "test:e2e": "NODE_ENV=test playwright test --project=e2e-tests --reporter=list",
    "test:api": "NODE_ENV=test playwright test --project=api-tests --reporter=list",
    "test:all": "NODE_ENV=test playwright test --reporter=list && NODE_ENV=test playwright test -c playwright-ct.config.ts --reporter=list"
  },
