# Local Development Environment Variables
# Copy this file to .env.local and update with your values

# Supabase Local Development
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=will_be_shown_after_supabase_start
SUPABASE_SERVICE_ROLE_KEY=will_be_shown_after_supabase_start

# Site URL
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Service Information
SERVICE_VERSION=1.0.0

# Redis URL (Optional for local development)
REDIS_URL="redis://localhost:6379"

# Email Service (Optional for local development)
RESEND_API_KEY=your_resend_api_key

# OAuth Credentials for Platform Integrations
# Google Ads
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_DEVELOPER_TOKEN=your_google_developer_token

# Meta (Facebook) Ads
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_BUSINESS_ID=your_meta_business_id

# Kakao Ads
KAKAO_CLIENT_ID=your_kakao_client_id
KAKAO_CLIENT_SECRET=your_kakao_client_secret

# Naver Ads  
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret

# Amazon Advertising
AMAZON_CLIENT_ID=your_amazon_client_id
AMAZON_CLIENT_SECRET=your_amazon_client_secret

# TikTok Ads
TIKTOK_APP_ID=your_tiktok_app_id
TIKTOK_APP_SECRET=your_tiktok_app_secret

# GitHub OAuth for Local Development (Disabled - not needed)
# GITHUB_CLIENT_ID=your_github_client_id
# GITHUB_CLIENT_SECRET=your_github_client_secret